@prefix : <http://ctuning.org/ml-benchmark-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
<http://ctuning.org/ml-benchmark-ontology> a owl:Ontology .

:AutomatedDesignSpaceExploration a owl:Class ;
    rdfs:subClassOf :Exploration .

:Standardization a owl:Class ;
    rdfs:subClassOf :Process .

:Workflow a owl:Class ;
    rdfs:subClassOf :Process .

:hasBenchmark a owl:ObjectProperty ;
    rdfs:domain :Model ;
    rdfs:range :Benchmark .

:mlperfInferencev1.0 a :MLPerfInference,
        owl:NamedIndividual .

:reproducibilityReportMLPerfInferencev1.1 a :ReproducibilityReport,
        owl:NamedIndividual .

:Exploration a owl:Class ;
    rdfs:subClassOf :Analysis .

:MLPerfInference a owl:Class ;
    rdfs:subClassOf :Benchmark .

:Report a owl:Class ;
    rdfs:subClassOf :Documentation .

:ReproducibilityReport a owl:Class ;
    rdfs:subClassOf :Report .

:Benchmark a owl:Class ;
    rdfs:subClassOf :Evaluation .

# For Code Diagrams
:CodeDiagram a :Report ;
    hasContentType 'image/png' .

# For Known Issues
:KnownIssues a :Benchmark ;
    hasDescription 'List of known issues' ;
    hasOwner 'NVIDIA' .

# For Heterogeneous MIG Workloads for Multi-MIG Systems
:HeterogeneousMIGWorkloadsForMultiMIGSystems a :Exploration ;
    hasBenchmark :mlperfInferencev1.0 .

# For How the HeteroMIG Harness is Designed
:HowTheHeteroMIGHarnessIsDesigned a :Process ;
    hasOwner 'NVIDIA' .

# For NVIDIA MLPerf Quantization
:NVIDIAMLPerfQuantization a :Benchmark ;
    hasDescription 'NVIDIA MLPerf benchmarks using quantization' ;
    hasOwner 'NVIDIA' .

# For Dell Submission Systems
:DellSubmissionSystems a :Workflow ;
    hasOwner 'Dell' .

# For GPU Implementations
:GPUImplementations a :Process ;
    hasOwner 'NVIDIA' .

# For Inside the Docker Container (refer to NVIDIA_for_detailed_instructions, including)
:InsideTheDockerContainer a :Workflow ;
    hasOwner 'NVIDIA' .

# For Before You Run Commands
:BeforeYouRunCommands a :Benchmark ;
    hasDescription 'Steps to take before running commands' ;
    hasOwner 'NVIDIA' .

# For Run Compliance Tests and Update Compliance Logs
:RunComplianceTestsAndUpdateComplianceLogs a :Process ;
    hasOwner 'NVIDIA' .

# For Triton Harness
:TritonHarness a :Benchmark ;
    hasDescription 'Using Triton for ML inference' ;
    hasOwner 'NVIDIA' .

# For NVIDIA Submissions
:NVIDIASubmissions a :Workflow ;
    hasOwner 'NVIDIA' .

# For Inside the Docker Container (refer to NVIDIA_for_detailed_instructions, including)
:InsideTheDockerContainer a :Workflow ;
    hasOwner 'NVIDIA' .

# For Run Triton Harness
:RunTritonHarness a :Benchmark ;
    hasDescription 'Steps to run Triton Harness' ;
    hasOwner 'NVIDIA' .

# For Multi-MIG Harness (refer_to_(DOCKER)*_Run_the_benchmark_)
:MultiMIGHarness a :Workflow ;
    hasOwner 'NVIDIA' .

# For SSD-Resnet34
:SSDResnet34 a :Benchmark ;
    hasDescription 'SSD-Resnet34 benchmarks' ;
    hasOwner 'NVIDIA' .

# For Run the BERT Benchmark
:RunTheBERTBenchmark a :Process ;
    hasOwner 'NVIDIA' .
